{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "blockchain_interact_tools",
  "version": "0.2.8",
  "identifier": "com.github.codermaybe",
  "build": {
    "beforeDevCommand": "npm run dev:vite",
    "devUrl": "http://localhost:9754",
    "beforeBuildCommand": "npm run build:vite",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "blockchain_interact_tools",
        "width": 1000,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
